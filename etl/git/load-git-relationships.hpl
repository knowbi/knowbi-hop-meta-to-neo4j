      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>if(!branch){branch = 'master'};

if(!branch.match(/\(\b[a-z0-9]+\/.*\)/) &amp;&amp; branch != 'master'){
	branch = 'master';
}

branch = branch.replace(branch.match(/\(/), '')
	.replace(branch.match(/\)/), '')
	.replace(/^.*[\\\/]/, '');

</jsScript_script>
